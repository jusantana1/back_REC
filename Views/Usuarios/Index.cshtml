@model CadUserModel

<h2>Criar Novo Usuario</h2>

@using (Html.BeginForm())
{
    <div>
        <label for="usua_nome">Nome</label>
        @Html.TextBoxFor(model => model.usua_nome)
    </div>
    <div>
        <label for="usua_email">Email</label>
        @Html.TextBoxFor(model => model.usua_email)
    </div>
    <div>
        <label for="usua_dt_cadastro">Telefone</label>
        @Html.TextBoxFor(model => model.usua_dt_cadastro)
    </div>
    <div>
        <input type="submit" value="Salvar" />
    </div>
}

